{
  "name": "galton",
  "version": "5.5.2",
  "description": "lightweight isochrone server",
  "author": "Stepan Kuzmin <to.stepan.kuzmin@gmail.com> (stepankuzmin.ru)",
  "license": "MIT",
  "main": "dist/bundle.js",
  "jsnext:main": "src/server.js",
  "scripts": {
    "build": "rollup -c",
    "docs": "documentation build src/* --format md --output docs/API.md",
    "lint": "eslint .",
    "start": "node index.js",
    "test": "node test/index.js"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "bin": {
    "galton": "./index.js"
  },
  "engine-strict": true,
  "engines": {
    "node": "4"
  },
  "dependencies": {
    "@turf/bezier": "3.7.0",
    "@turf/destination": "3.7.0",
    "@turf/point-grid": "3.7.0",
    "concaveman": "1.1.1",
    "kcors": "2.2.0",
    "koa": "2.0.0-alpha.7",
    "koa-compress": "2.0.0",
    "koa-conditional-get": "2.0.0",
    "koa-etag": "3.0.0",
    "koa-logger": "2.0.0",
    "minimist": "1.2.0",
    "osrm": "5.5.2"
  },
  "devDependencies": {
    "documentation": "4.0.0-beta16",
    "eslint": "3.13.0",
    "eslint-config-airbnb-base": "11.0.0",
    "eslint-plugin-import": "2.2.0",
    "@mapbox/geojsonhint": "2.0.0",
    "pre-commit": "1.2.2",
    "rollup": "0.40.2",
    "rollup-plugin-async": "1.2.0",
    "rollup-plugin-commonjs": "7.0.0",
    "rollup-plugin-json": "2.1.0",
    "rollup-plugin-node-resolve": "2.0.0",
    "supertest-koa-agent": "0.3.0",
    "tape": "4.6.3"
  }
}
