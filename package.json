{
  "name": "galton",
  "version": "1.3.8",
  "description": "lightweight isochrone server",
  "main": "src/index.js",
  "author": "Stepan Kuzmin <to.stepan.kuzmin@gmail.com> (stepankuzmin.ru)",
  "license": "MIT",
  "homepage": "https://github.com/urbica/galton#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/urbica/galton.git"
  },
  "bugs": {
    "url": "https://github.com/urbica/galton/issues"
  },
  "scripts": {
    "build": "babel -d dist src",
    "docs": "documentation build src/* --format md --output docs/API.md",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "eslint .",
    "prepublish": "npm run build && npm run docs",
    "start": "node index.js",
    "test": "tape -r babel-register test/index.js"
  },
  "pre-commit": [
    "flow",
    "lint",
    "test"
  ],
  "bin": {
    "galton": "./index.js"
  },
  "engine-strict": true,
  "engines": {
    "node": "4"
  },
  "dependencies": {
    "@turf/bezier": "3.5.2",
    "@turf/destination": "3.5.2",
    "@turf/point-grid": "3.5.2",
    "babel-runtime": "6.11.6",
    "concaveman": "1.1.1",
    "kcors": "2.2.0",
    "koa": "2.0.0-alpha.7",
    "koa-compress": "2.0.0",
    "koa-conditional-get": "2.0.0",
    "koa-etag": "3.0.0",
    "koa-logger": "2.0.0",
    "minimist": "1.2.0",
    "npid": "0.4.0",
    "osrm": "5.4.1"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-eslint": "7.0.0",
    "babel-plugin-syntax-async-functions": "6.13.0",
    "babel-plugin-transform-flow-strip-types": "6.14.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-register": "6.16.3",
    "documentation": "4.0.0-beta11",
    "eslint": "3.8.1",
    "eslint-config-airbnb-base": "9.0.0",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-import": "2.0.1",
    "flow-bin": "0.33.0",
    "pre-commit": "1.1.3",
    "supertest-koa-agent": "0.3.0",
    "tape": "4.6.2"
  }
}
